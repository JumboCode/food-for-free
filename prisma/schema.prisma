generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ChangeMe {
  id        Int      @id @default(autoincrement())
  text      String
  createdAt DateTime @default(now())
}

model SophieJuliaMessage {
  id        Int      @id @default(autoincrement())
  text      String
  createdAt DateTime @default(now())
}

model yuvitSaraMessage{
  id        Int      @id @default(autoincrement())
  text      String
  createdAt DateTime @default(now())
}

model YuvitSaraMessage {
  id        Int      @id @default(autoincrement())
  text      String
  createdAt DateTime @default(now())
}